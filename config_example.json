            "listen": [
                80,
                443
            ],
            "locations": {
                "/": {
                    "proxy_pass": "http://localserver.tld:80"
                }
            },
            "subdomains": [
                {
                    "id": "banana.4353.uh.edu",
                    "locations": {
                        "/banana": {
                            "serve": "/home/ngavax/html/banana.html"
                        }
                    }
                    
                },
                {
                    "id": "*.4353.uh.edu",
                    "locations": {
                        "/": {
                            "serve": "/home/ngavax/html/404.html"
                        }
                    }
                }
            ]
        },
        {
            "id": "uhhh.edu",
            "listen": [
                80,
                443
            ],
            "locations": {
                "/": {
