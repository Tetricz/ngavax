{
    "domains": [
        {
            "id": "4353.uh.edu",
            "listen": [
                80,
                443
            ],
            "locations": [
                {
                    "directory": "/",
                    "proxy_pass": "http://localserver.tld:80"
                }
            ],
            "subdomains": [
                {
                    "id": "banana.4353.uh.edu",
                    "locations": [
                        {
                            "directory": "/banana",
                            "serve": "/home/ngavax/html/banana.html"
                        }
                    ]
                },
                {
                    "id": "*.4353.uh.edu",
                    "locations": [
                        {
                            "directory": "/",
                            "serve": "/home/uh.edu/html/404.html"
                        }
                    ]
                }
            ]
        },
        {
            "id": "uhhh.edu",
            "listen": [
                80,
                443
            ],
            "locations": [
                {
                    "directory": "/",
                    "serve": "/home/uh.edu/html/"
                },
                {
                    "directory": "/ftp",
                    "serve": "/home/uh.edu/ftp/",
                    "autoindex": true
                },
                {
                    "directory": "/pass",
                    "proxy_pass": "192.168.1.55:80"
                }
            ]
        }
    ]
}